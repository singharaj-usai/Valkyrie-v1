{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node@2.5.10"
      },
      {
        "src": "client/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ]
  }